{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=DeviceSpecAnalyzer.db"
  },
  "RepositoryWatcher": {
    "RepositoryPath": "./SpecificationRepository",
    "DebounceDelayMs": 2000,
    "ProcessExistingFiles": true,
    "SupportedExtensions": [ ".pdf" ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "DeviceSpecAnalyzer": "Information"
    }
  },
  "AllowedHosts": "*"
}